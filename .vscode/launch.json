{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "OctoPrint",
			"type": "python",
			"request": "launch",
			"module": "octoprint",
			"console": "integratedTerminal",
			"justMyCode": true,
			"args": [
				"serve",
				"--debug",
				"--host=127.0.0.1",
				"--port=8080"
			],
			"cwd": "${workspaceFolder}/src",
			"preLaunchTask": "clean build artifacts",
		},
	]
}
